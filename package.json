{
  "name": "mobooru",
  "author": "just nobody",
  "version": "0.1.0",
  "description": "a mobile-friendly image browser",
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.18.0",
    "express": "^4.14.0",
    "gm": "^1.23.0",
    "mongodb": "^2.2.16",
    "multer": "^1.2.1"
  },
  "scripts": {
    "dev:webpack": "cross-env NODE_ENV=development webpack --config web/webpack.config.js --watch",
    "dev:nodemon": "nodemon -r babel-register server/app/main",
    "dev:bsync": "browser-sync start --proxy localhost:8080 --files web/build/** web/index.html --reload-delay 1000",
    "dev:db": "mongod --dbpath ./data/db",
    "dev": "run-p dev:*",
    "build:web": "cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "build": "run-p build:*",
    "start": "node -r babel-register server/app/main"
  },
  "engines": {
    "node": "6.9.x"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-babili": "0.0.9",
    "babel-preset-env": "^1.1.4",
    "babili-webpack-plugin": "0.0.7",
    "mdi": "^1.7.22",
    "normalize.css": "^5.0.0",
    "vue": "^2.1.8",
    "browser-sync": "^2.18.5",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
